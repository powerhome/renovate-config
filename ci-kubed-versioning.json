{
    "regexManagers": [
        {
            "fileMatch": [
                "Jenkinsfile$"
            ],
            "matchStrings": [
                "library 'github.com/powerhome/ci-kubed@(?<currentValue>.*?)'"
            ],
            "datasourceTemplate": "github-releases",
            "depNameTemplate": "powerhome/ci-kubed"
        }
    ],
    "packageRules": [
        {
            "matchManagers": ["regex"],
            "matchDepNames": ["powerhome/ci-kubed"],
            "prHourlyLimit": 0,
            "prConcurrentLimit": 0
        }
    ]
}
